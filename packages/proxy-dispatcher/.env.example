# SOCKS5 proxy configuration
SOCKS5_BIND_HOST=127.0.0.1
SOCKS5_MIN_PORT=10000
SOCKS5_MAX_PORT=15000

# Number of attempts to find an available port
PORT_ALLOCATION_ATTEMPTS=10
